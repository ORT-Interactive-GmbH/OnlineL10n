platform :ios, '8.0'

def sharedPods
   pod 'ReactiveObjC'
end

target "SwiftLocalization" do
   sharedPods
end

target "SwiftLocalizationUITests" do
   sharedPods
end

target "OnlineL10n" do
   sharedPods
end

target "OnlineL10nTests" do
   sharedPods
end

use_frameworks!

# ignore warnings in pod framework compile
inhibit_all_warnings!
